#!/bin/sh

# VIDEO INPUT
videocard="/dev/video0"
videocardtype="mjpeg"

# AUDIO INPUT
audiocard="hw:CARD=MS2109,DEV=0"

# VIDEO OUTPUT
videosize="1280x720"
encoder="v4l2m2m"
preset="veryfast"
framerate="30"
keyframe="60"
bitrate="5000k"
outputformat="yuv420p"

#AUDIO OUTPUT
bitrateaudio="128k"
audioencoder="aac"
audiochannels="2"
sampling="48000"
# RTMP DETAILS
ingesturl="rtmp://rtmp.server.com"
ingestkey="truc"
streamkey="$ingesturl/$ingestkey"
